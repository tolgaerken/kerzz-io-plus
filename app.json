{
  "expo": {
    "name": "kerzz-io-plus",
    "slug": "kerzz-io-plus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./logos/ios/Icon-1024.png",
    "scheme": "kerzzioplus",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kerzz.kerzz_io",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./logos/android/ic_launcher_192.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./logos/android/ic_launcher_192.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.kerzz.kerzz_io",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "output": "static",
      "favicon": "./logos/ios/Icon-1024.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./logos/ios/Icon-1024.png",
          "color": "#3B82F6",
          "defaultChannel": "default",
          "sounds": []
        }
      ],
      [
        "@react-native-firebase/app",
        {
          "platforms": ["ios", "android"]
        }
      ]
    ],
    "notification": {
      "icon": "./logos/ios/Icon-1024.png",
      "color": "#3B82F6"
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
